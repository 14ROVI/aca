.memory
    len: .int 5
    a: .int 1, 1, 1, 1, 1
    b: .int 1, 2, 3, 4, 5
    c: .space 20

.instructions
    main:
        li $1 0
        lw $2 $0 len
    
    loop:
        bge $1 $2 exit
        
        lsft $12 $1 2 // index
        lw $9 $12 a // load a
        lw $10 $12 b // load b
        add $11 $9 $10 // add
        sw $11 $12 c // set c
        
        addi $1 $1 1

        j loop

    exit: